{% extends admin.getTemplate('base_list_field') %}

{% block field %}
    <i class="fa fa-youtube-play {% if (not object.isFlash) %}text-muted{% endif %}"></i>
    <i class="fa fa-file-image-o {% if (not object.isImage) %}text-muted{% endif %}"></i>
    <i class="fa fa-file-code-o  {% if (not object.isHtml) %}text-muted{% endif %}"></i>
{% endblock %}
